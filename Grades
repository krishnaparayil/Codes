
#include<stdio.h>
void main()
{
  int mark,grade;
printf("enter the mark");
scanf("%d",&mark);
if((mark>=90)&&(mark<=100)) 
printf("grade is A+"); 
if((mark>=80)&&(mark<=89)) 
printf("grade is A"); 
if((mark>=70)&&(mark<=79)) 
printf("grade is B"); 
if((mark>=60)&&(mark<=69)) 
printf("grade is C"); 
if((mark>=50)&&(mark<=59)) 
printf("grade is D"); 
if((mark>=0)&&(mark<=49)) 
printf("grade is F"); 
else
printf("invalid mark");



}
